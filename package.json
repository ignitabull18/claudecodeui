{
  "name": "claude-code-ui",
  "version": "1.5.0",
  "description": "A web-based UI for Claude Code CLI",
  "type": "module",
  "main": "server/index.js",
  "scripts": {
    "dev": "concurrently --kill-others \"bun run server\" \"bun run client\"",
    "server": "bun server/index.js",
    "client": "vite --host",
    "build": "vite build",
    "preview": "vite preview",
    "start": "bun run build && bun run server",
    "audit:claude-compliance": "node scripts/audit-claude-compliance.js",
    "lint": "eslint . --fix",
    "lint:check": "eslint .",
    "prepare": "husky install"
  },
  "keywords": [
    "claude",
    "ai",
    "code",
    "ui",
    "assistant"
  ],
  "author": "Claude Code UI Contributors",
  "license": "MIT",
  "dependencies": {
    "@babel/parser": "^7.25.8",
    "@babel/traverse": "^7.25.8",
    "@babel/types": "^7.25.8",
    "@codemirror/lang-css": "^6.3.1",
    "@codemirror/lang-html": "^6.4.9",
    "@codemirror/lang-javascript": "^6.2.4",
    "@codemirror/lang-json": "^6.0.1",
    "@codemirror/lang-markdown": "^6.3.3",
    "@codemirror/lang-python": "^6.2.1",
    "@codemirror/theme-one-dark": "^6.1.2",
    "@tailwindcss/typography": "^0.5.16",
    "@uiw/react-codemirror": "^4.23.13",
    "@xterm/addon-clipboard": "^0.1.0",
    "@xterm/addon-webgl": "^0.18.0",
    "acorn": "^8.14.0",
    "acorn-walk": "^8.3.4",
    "bcrypt": "^6.0.0",
    "chokidar": "^4.0.3",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cors": "^2.8.5",
    "escodegen": "^2.1.0",
    "eslint": "^9.17.0",
    "espree": "^10.3.0",
    "esprima": "^4.0.1",
    "express": "^4.18.2",
    "glob": "^11.0.3",
    "jscodeshift": "^17.3.0",
    "jsonwebtoken": "^9.0.2",
    "lucide-react": "^0.515.0",
    "madge": "^8.0.0",
    "mime-types": "^3.0.1",
    "multer": "^2.0.1",
    "node-cron": "^3.0.3",
    "node-fetch": "^2.7.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-dropzone": "^14.2.3",
    "react-markdown": "^10.1.0",
    "react-router-dom": "^6.8.1",
    "tailwind-merge": "^3.3.1",
    "ts-morph": "^24.0.0",
    "typescript": "^5.7.3",
    "ws": "^8.14.2",
    "xterm": "^5.3.0",
    "xterm-addon-fit": "^0.8.0"
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "eslint --fix",
      "node scripts/audit-claude-compliance.js"
    ],
    "server/routes/*.js": [
      "node scripts/audit-claude-compliance.js"
    ]
  },
  "devDependencies": {
    "@stagewise-plugins/react": "^0.6.2",
    "@stagewise/toolbar-react": "^0.6.2",
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@vitejs/plugin-react": "^4.6.0",
    "autoprefixer": "^10.4.16",
    "concurrently": "^8.2.2",
    "husky": "^9.1.7",
    "lint-staged": "^16.1.2",
    "postcss": "^8.4.32",
    "sharp": "^0.34.2",
    "tailwindcss": "^3.4.0",
    "vite": "^7.0.4"
  }
}
